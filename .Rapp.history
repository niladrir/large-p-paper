mean.samp <- function(dat, no.samp, method){#
	dat1 <- NULL#
for (i in 1:length(unique(dat$.sample))){#
	dat1 <- rbind(dat1, data.frame(PlotNo = i, means = mean_met(dat[dat$.sample == i,], no.samp, method)))#
}#
return(dat1)#
}#
